{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "$types/*": ["types/*"],
      "$components/*": ["components/*"]
    }
  }
}
